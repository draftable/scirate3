# Ignore bundler config
/.bundle
/bundler_stubs/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/sphinx

# Ignore all logfiles and tempfiles.
/log/*.log
/log/*.pid
/tmp
/coverage

# Ignore local config files
config/database.yml
config/thinking_sphinx.yml
config/*.sphinx.conf
local_settings.rb

# extra ignores
*.log
doc/
*.swp
*.swo
*~
.project
.DS_Store
backup/*.dump

# This is deployment-specific
public/robots.txt
